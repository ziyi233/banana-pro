{
  "name": "koishi-plugin-banana-pro",
  "description": "独立webui，可在线拉取预设，支持 DALL-E 和 ChatLuna 的 AI 图片生成插件",
  "version": "1.4.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "author": "ziyi233",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziyi233/koishi-plugin-banana-pro.git"
  },
  "bugs": {
    "url": "https://github.com/ziyi233/koishi-plugin-banana-pro/issues"
  },
  "homepage": "https://github.com/ziyi233/koishi-plugin-banana-pro#readme",
  "files": [
    "lib",
    "dist",
    "client"
  ],
  "license": "MIT",
  "scripts": {
    "build:server": "tsc",
    "build:client": "koishi-console build",
    "build": "yarn build:server && yarn build:client"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "ai",
    "image-generation",
    "dalle",
    "chatluna"
  ],
  "dependencies": {
    "@langchain/core": "^0.3.62"
  },
  "devDependencies": {
    "@koishijs/client": "^5.30.0",
    "@koishijs/plugin-console": "^5.30.0",
    "koishi": "^4.18.7",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@koishijs/plugin-console": "^5.30.0",
    "koishi": "^4.18.0"
  },
  "optionalDependencies": {
    "koishi-plugin-chatluna": "^1.3.0"
  },
  "koishi": {
    "description": {
      "zh": "支持 DALL-E 和 ChatLuna 的 AI 图片生成插件"
    },
    "service": {
      "required": [
        "database",
        "http",
        "logger",
        "i18n"
      ],
      "optional": [
        "chatluna",
        "monetary",
        "console"
      ]
    },
    "locales": [
      "zh-CN"
    ],
    "browser": true,
    "icon": "mdi:image-multiple"
  }
}
